{
  "name": "league-of-legends",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3007 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && npm run build:electron && electron-builder",
    "build:electron": "tsc -p electron/tsconfig.json",
    "dist": "npm run build && npm run build:electron && electron-builder --publish=never"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.3",
    "tailwindcss": "^4",
    "to-ico": "^1.1.5",
    "typescript": "^5",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.riotgames.leagueoflegends",
    "productName": "League of Legends",
    "directories": {
      "output": "release"
    },
    "files": [
      "out/**/*",
      "dist-electron/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.games"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
